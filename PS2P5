<?xml version="1.0"?>
<flowgorithm fileversion="2.11">
    <attributes>
        <attribute name="name" value=""/>
        <attribute name="authors" value="PhotonUser"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2024-02-24 11:36:37 AM"/>
        <attribute name="created" value="UGhvdG9uVXNlcjtFQzJBTUFaLVZLU05SR1A7MjAyNC0wMi0yNDsxMTowNzozMiBBTTszMzg5"/>
        <attribute name="edited" value="UGhvdG9uVXNlcjtFQzJBTUFaLVZLU05SR1A7MjAyNC0wMi0yNDsxMTozNjozNyBBTTsxOzM1MDQ="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="price" type="Real" array="False" size=""/>
            <declare name="discount" type="Real" array="False" size=""/>
            <declare name="savings" type="Integer" array="False" size=""/>
            <declare name="discountprice" type="Real" array="False" size=""/>
            <input variable="price"/>
            <input variable="discount"/>
            <assign variable="savings" expression="price*discount"/>
            <assign variable="discountprice" expression="price-savings"/>
            <output expression="price*discount &amp; price-savings" newline="True"/>
        </body>
    </function>
</flowgorithm>
